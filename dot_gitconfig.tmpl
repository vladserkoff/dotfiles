[user]
    name = "{{ .name }}"
    email = "{{ .email }}"
[core]
	editor = micro
[pull]
	rebase = true
[init]
	defaultBranch = main
[color]
    ui = auto
[core]
    autocrlf = input
    excludesfile = ~/.gitignore_global
[filter "lfs"]
    process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
